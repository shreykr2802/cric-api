{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "rootDir": "api",
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["api/**/*"],
  "exclude": ["node_modules"]
}
